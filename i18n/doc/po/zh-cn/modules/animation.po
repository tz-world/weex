"<script>\n"
"  module.exports = {\n"
"    ready: function () {\n"
"      var animation = require('@weex-module/animation');\n"
"      var testEl = this.$el('test');\n"
"      animation.transition(testEl, {\n"
"        styles: {\n"
"          color: '#FF0000',\n"
"          transform: 'translate(1, 1)'\n"
"        },\n"
"        duration: 0, //ms\n"
"        timingFunction: 'ease',\n"
"        transform-origin: 'center center',\n"
"        delay: 0 //ms\n"
"      }, function () {\n"
"        nativeLog('animation finished.')\n"
"      })\n"
