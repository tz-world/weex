#, no-wrap
msgid ""
"  window.weex.init({\n"
"    jsonpCallback: JSONP_CALLBACK_NAME,\n"
"    appId: location.href,\n"
"    source: page,\n"
"    loader: loader,\n"
"    rootId: 'weex'\n"
"  })\n"
"}\n"
msgstr ""
"  window.weex.init({\n"
